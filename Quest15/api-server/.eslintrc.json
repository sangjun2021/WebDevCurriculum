{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:import/typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "class-methods-use-this" : "warn",
        "consistent-return" : "warn",
        "no-param-reassign": "warn",
        "no-alert" :"warn",
        "no-new" : "warn",
        "max-len" :"off",
        "no-restricted-globals":"warn",
        "array-callback-return" : "warn",
        "no-await-in-loop" : "off"
        
    }
}
